    "@testing-library/svelte": "^3.1.3",
    "babel-jest": "^28.1.3",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.1",
    "jest": "^28.1.3",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^28.1.3",
